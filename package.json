{
  "name": "hiddout-client",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "build": "node ./devScript/prebuild.js && webpack --config config/webpack.prod.js && node ./devScript/dist.js",
    "flow": "flow",
    "lint": "eslint --ext .js src",
    "lint-fix": "eslint --ext .js src --fix",
    "dev": "node ./devScript/prebuild.js && webpack-dev-server --open --config config/webpack.dev.js",
    "test": "npm run unit-testing && npm run flow check && npm run lint && npm run build",
    "unit-testing": "mocha test/**/*.test.js --require @babel/register"
  },
  "prettier": {
    "useTabs": true,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "parser": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiddout/hiddout-client.git"
  },
  "author": "Tyreal Gray",
  "license": "AFL-3.0",
  "bugs": {
    "url": "https://github.com/hiddout/hiddout-client/issues"
  },
  "homepage": "https://github.com/hiddout/hiddout-client#readme",
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.6.2",
    "archiver": "^3.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "circular-dependency-plugin": "5.2.0",
    "core-js": "3.3.5",
    "css-loader": "3.0.0",
    "eslint": "5.16.0",
    "eslint-plugin-react": "7.12.4",
    "expect.js": "0.3.1",
    "file-loader": "3.0.1",
    "flow-bin": "0.110.0",
    "fs-extra": "8.1.0",
    "hiddout-viewer": "0.1.12",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "6.2.2",
    "prettier": "1.17.0",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "0.23.1",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "connected-react-router": "6.5.1",
    "i18next": "16.0.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-markdown": "^4.0.6",
    "react-redux": "7.0.3",
    "react-router-dom": "5.1.2",
    "redux": "4.0.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "0.87.2"
  }
}
